VAR 
    a : INTEGER;
    b : INTEGER;
    c : STRING;
    d : STRING;
    PROCEDURE hola(E INTEGER a)
    BEGIN
	WRITELN("El valor de a es " + STR(a));
    END
    

BEGIN
    a <-- 2;
    WRITELN(STR(a));
    b <-- 4;
    WRITELN(STR(b));
    c <-- "Hola";
    d <-- "Mundo";
    WRITELN(c+d);
    CALL PROCEDURE hola(a);
END



VAR 
    a : INTEGER;
    PROCEDURE hola(E INTEGER a)
    BEGIN
	WRITELN("El valor de a es " + STR(a));
    END
BEGIN
    WRITELN("hola");
    CALL PROCEDURE hola(a);
END




VAR 
a:INTEGER;
BEGIN
WRITELN("hola");
END



print("Llamo el run con \n", tabla)
        print(variables)
        print(padre)